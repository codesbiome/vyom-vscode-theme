{
  "$schema": "vscode://schemas/color-theme",
  "name": "Vyom Theme",
  "type": "dark",
  "colors": {
    "foreground": "#bbbbbb",
    "errorForeground": "#ff0000",
    "focusBorder": "#00000000",
    "editorCursor.foreground": "#ffb300da",
    "editorError.foreground": "#ff000000", // Remove

    "editor.background": "#1e1e1e",
    "editor.foreground": "#bbbbbb",
    "editor.selectionBackground": "#2c507884",
    "editor.selectionHighlightBackground": "#2c5078b5",
    "editor.inactiveSelectionBackground": "#2c507884",
    "editor.lineHighlightBackground": "#22272cad",
    "editor.wordHighlightBackground": "#2c507884",

    "tab.activeBackground": "#1e1e1e",
    "tab.activeForeground": "#bbbbbb",
    "tab.inactiveBackground": "#23282f",
    "tab.inactiveForeground": "#adbac5bc",
    "tab.border": "#28323d",
    "tab.hoverForeground": "#bbbbbb",
    "tab.activeModifiedBorder": "#bb4476",
    "tab.inactiveModifiedBorder": "#bb447683",
    "editorGroupHeader.tabsBackground": "#1e2329",

    "breadcrumb.background": "#1e1e1e",
    "breadcrumb.foreground": "#bbbbbb",
    "breadcrumb.activeSelectionForeground": "#e1699b",

    "activityBar.activeBackground": "#1e262f9c",
    "activityBar.activeBorder": "#b9cde8",
    "activityBar.background": "#28323d",
    "activityBar.inactiveForeground": "#6b7f94f8",
    "activityBar.foreground": "#b9cde8",
    "activityBar.activeFocusBorder": "#fe7ab2",
    "activityBar.dropBorder": "#bb4476",
    // "activityBar.border": "#303d4a",

    "activityBarBadge.background": "#bb4476",
    "activityBarBadge.foreground": "#ffffff",

    "badge.background": "#b35118d8",
    "badge.foreground": "#ffffffbe",

    "button.background": "#2e3c4b",
    "button.foreground": "#ffffffe3",
    "button.border": "#3d4854",

    "dropdown.foreground": "#879eb7",
    "dropdown.background": "#171717",
    "dropdown.border": "#4141417c",
    "dropdown.listBackground": "#171717",

    "editorBracketMatch.border": "#4e6374",
    "editorBracketMatch.background": "#242424",

    "sideBar.background": "#232527",
    "sideBar.foreground": "#c4d0dcde",
    "sideBar.dropBackground": "#272f3a",

    "sideBarTitle.foreground": "#bbbbbbd4",
    "sideBarSectionHeader.background": "#28323d",
    "sideBarSectionHeader.foreground": "#7d9dbe",
    "sideBarSectionHeader.border": "#00000041",

    "titleBar.activeBackground": "#29333e",
    "titleBar.activeForeground": "#c5ced8",
    "titleBar.inactiveBackground": "#28323d",
    "titleBar.inactiveForeground": "#adbac5bc",

    "list.activeSelectionBackground": "#3f312891",
    "list.activeSelectionForeground": "#aca490",
    "list.errorForeground": "#cf7c7c",
    "list.focusBackground": "#b35118d8",
    "list.focusForeground": "#cfcfcf",
    "list.highlightForeground": "#de7e50",
    "list.hoverBackground": "#2e2e2ec4",
    "list.inactiveFocusBackground": "#3f312891",
    "list.inactiveSelectionBackground": "#3f312891",
    "list.inactiveSelectionForeground": "#aca490",
    "list.warningForeground": "#d39a69"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment", "punctuation.definition.generic"],
      "settings": {
        "foreground": "#7f8c98"
      }
    },
    {
      "name": "String Quoted",
      "scope": ["string.quoted"],
      "settings": {
        "foreground": "#fe8170"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "storage.type.function.arrow",
        "keyword.operator.assignment",
        "variable.other.readwrite",
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#ffffffcb"
      }
    },
    {
      "name": "Punctuation Separator",
      "scope": [
        "punctuation.separator.inheritance",
        "meta.class meta.function-call",
        "keyword.other.array.phpdoc",
        "bracket.round meta.function-call",
        "comment.block.documentation punctuation.definition.bracket.curly"
      ],
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ff8c69"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.other",
        "keyword.operator.new",
        "storage.type",
        "storage.modifier",
        "variable.language.this",
        "variable.language.this punctuation.definition.variable.php",
        "variable.language.super",
        "keyword.other.typedef",
        "constant.language",
        "constant.other",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#fe7ab2"
      }
    },
    {
      "name": "Comment Doc Keyword",
      "scope": ["keyword.other.phpdoc", "comment.block.documentation storage.type.class"],
      "settings": {
        "foreground": "#DD9AC5"
      }
    },
    {
      "name": "Class Declaration",
      "scope": ["entity.name.type.class"],
      "settings": {
        "foreground": "#6bdfff"
      }
    },
    {
      "name": "Class Support",
      "scope": [
        "support.class",
        "support.type.property-name",
        "entity.name.type.module",
        "entity.other.inherited-class",
        "entity.name.class",
        "meta.return.type entity.name.type",
        "comment.block.documentation entity.name.type.instance",
        "entity.name.label"
      ],
      "settings": {
        "foreground": "#d9baff"
      }
    },
    {
      "name": "Class Methods",
      "scope": ["entity.name.function", "punctuation.definition.variable.php", "meta.type.annotation entity.name.type"],
      "settings": {
        "foreground": "#4DB1CB"
      }
    },
    {
      "name": "Class Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "entity.name.type.module"
      ],
      "settings": {
        "foreground": "#ffbf67e0"
      }
    },
    {
      "name": "Functions & Predefined Symbols",
      "scope": [
        "support.function",
        "variable.function",
        "keyword.other.special-method",
        "constant.other.php",
        "variable.other.constant.object"
      ],
      "settings": {
        "foreground": "#96b8e9"
      }
    },
    {
      "name": "Function Parameter (Arguments)",
      "scope": ["variable.other", "text.html.vue-html variable.other.readwrite"],
      "settings": {
        "foreground": "#6cc1c1"
      }
    },
    {
      "name": "HTML Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#DD9AC5"
      }
    },
    {
      "name": "HTML Tag Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ffbf67e0"
      }
    }
  ]
}
