{
  "$schema": "vscode://schemas/color-theme",
  "name": "Vyom Theme",
  "type": "dark",
  "colors": {
    "foreground": "#bbbbbb",
    "focusBorder": "#55779370",
    "editorCursor.foreground": "#ffb300da",
    "editorError.foreground": "#ff000000", // Remove

    "editor.background": "#1e1e1e",
    "editor.foreground": "#bbbbbb",
    "editor.selectionBackground": "#35434f82",
    "editor.selectionHighlightBackground": "#35434f5b",
    "editor.inactiveSelectionBackground": "#35434f5b",
    "editor.lineHighlightBackground": "#22272cad",
    "editor.wordHighlightBackground": "#2c507884",

    "tab.activeBackground": "#1e1e1e",
    "tab.activeForeground": "#bbbbbb",
    "tab.inactiveBackground": "#272c2f",
    "tab.inactiveForeground": "#9ca9b1",
    "tab.border": "#1b1f23c4",
    "tab.hoverForeground": "#bbbbbb",
    "tab.activeModifiedBorder": "#556777e6",
    "tab.inactiveModifiedBorder": "#55677781",
    "editorGroupHeader.tabsBackground": "#242628",

    "breadcrumb.background": "#1e1e1e",
    "breadcrumb.foreground": "#bbbbbb",
    "breadcrumb.activeSelectionForeground": "#e1699b",

    "activityBar.activeBackground": "#272c2f",
    "activityBar.activeBorder": "#c7d4e5e8",
    "activityBar.background": "#2b3034",
    "activityBar.inactiveForeground": "#c7d4e56f",
    "activityBar.foreground": "#c7d4e5e8",
    "activityBar.activeFocusBorder": "#fe7ab2",
    "activityBar.dropBorder": "#bb4476",

    "activityBarBadge.background": "#bb4476",
    "activityBarBadge.foreground": "#ffffffdd",

    "badge.background": "#bb4476",
    "badge.foreground": "#ffffffdd",

    "button.background": "#2d3841",
    "button.foreground": "#ffffffcf",

    "dropdown.foreground": "#879eb7",
    "dropdown.background": "#171717",
    "dropdown.border": "#4141417c",
    "dropdown.listBackground": "#171717",

    "editorBracketMatch.border": "#4e6374",
    "editorBracketMatch.background": "#242424",

    "sideBar.background": "#232527",
    "sideBar.foreground": "#c4d0dcde",
    "sideBar.dropBackground": "#272f3a",

    "sideBarTitle.foreground": "#bbbbbbd4",
    "sideBarSectionHeader.background": "#272c2f",
    "sideBarSectionHeader.foreground": "#92a3b1",
    "sideBarSectionHeader.border": "#00000041",

    "titleBar.activeBackground": "#31363a",
    "titleBar.activeForeground": "#c5ced8",
    "titleBar.inactiveBackground": "#2b3034",
    "titleBar.inactiveForeground": "#adbac5bc",

    "list.activeSelectionBackground": "#35434fbd",
    "list.activeSelectionForeground": "#b9cfe2ea",
    "list.focusOutline": "#35434f66",
    "list.focusBackground": "#35434f66",
    "list.inactiveSelectionBackground": "#35434f92",
    "list.inactiveFocusBackground": "#35434f92",
    "list.inactiveSelectionForeground": "#b9cfe2ea",
    "list.hoverBackground": "#35434f66",
    "list.errorForeground": "#ff7fb4",

    "settings.rowHoverBackground": "#232527cc",
    "settings.focusedRowBackground": "#232527",
    "settings.focusedRowBorder": "#5577933c",
    "settings.modifiedItemIndicator": "#596977",
    "settings.headerForeground": "#92a3b1",
    "settings.headerBorder": "#262626",
    "settings.dropdownBackground": "#171717",
    "settings.dropdownBorder": "#262626",
    "settings.dropdownForeground": "#a6b5c2e6",

    "textLink.foreground": "#8ac0f6c4",
    "textLink.activeForeground": "#8ac0f6e7",
    "textPreformat.foreground": "#e08baee0",

    // Scroll Bar
    "scrollbar.shadow": "#111111b2",

    // Scroll Bar Slider
    "scrollbarSlider.background": "#3e485180",
    "scrollbarSlider.hoverBackground": "#3e4851b3",
    "scrollbarSlider.activeBackground": "#4b5763bb",

    "icon.foreground": "#d3d8deb3",

    "input.background": "#171717",
    "input.foreground": "#a6b5c2e6",
    "input.placeholderForeground": "#a6b5c27a",

    "statusBar.background": "#913a5f",
    "statusBar.noFolderBackground": "#913a5f",
    "statusBar.debuggingBackground": "#a52f2f",

    "editor.findMatchHighlightBackground": "#4a719392",
    "editorInlayHint.background": "#252525e1",
    "editorInlayHint.foreground": "#8f9ca8c0",

    "editorSuggestWidget.background": "#232527",
    "editorSuggestWidget.border": "#5577933c",
    "editorSuggestWidget.foreground": "#a6b3c1de",
    "editorSuggestWidget.highlightForeground": "#c58961",
    "editorSuggestWidget.selectedBackground": "#35434fbd",
    "editorSuggestWidget.focusHighlightForeground": "#79baffde",

    "gitDecoration.modifiedResourceForeground": "#ffbf67e0",
    "gitDecoration.untrackedResourceForeground": "#63ececba",
    "gitDecoration.addedResourceForeground": "#75bcf2ed",
    "gitDecoration.deletedResourceForeground": "#ff9797ed",
    "gitDecoration.conflictingResourceForeground": "#ff98c4ed",
    "gitDecoration.submoduleResourceForeground": "#699bdb",

    "menu.background": "#232527",
    "menu.border": "#5577933c",
    "menu.foreground": "#c4d0dcde",
    "menu.selectionBackground": "#35434fbd",
    "menu.selectionForeground": "#b9cfe2ea",
    "menu.separatorBackground": "#5577933c",

    "peekView.border": "#55779359",
    "peekViewEditor.background": "#282b2d",
    "peekViewTitle.background": "#232527",
    "peekViewEditor.matchHighlightBackground": "#4a719392",
    "peekViewResult.background": "#232527",
    "peekViewResult.fileForeground": "#a3a3a3",
    "peekViewResult.lineForeground": "#a3a3a3",
    "peekViewResult.selectionBackground": "#35434fbd",
    "peekViewResult.selectionForeground": "#b9cfe2ea",
    "peekViewTitleDescription.foreground": "#8ac0f6c4",
    "peekViewTitleLabel.foreground": "#c4d0dcde",
    "peekViewResult.matchHighlightBackground": "#4a719392",

    "symbolIcon.arrayForeground": "#9c9c9c",
    "symbolIcon.booleanForeground": "#cf7c7c",
    "symbolIcon.classForeground": "#59ae8b",
    "symbolIcon.constantForeground": "#cf7c7c",
    "symbolIcon.constructorForeground": "#d39a69",
    "symbolIcon.fieldForeground": "#92b4b1",
    "symbolIcon.functionForeground": "#c1a860",
    "symbolIcon.interfaceForeground": "#3a9c93",
    "symbolIcon.methodForeground": "#c1a860",
    "symbolIcon.moduleForeground": "#9c9c9c",
    "symbolIcon.namespaceForeground": "#d39a69",
    "symbolIcon.numberForeground": "#cf7c7c",
    "symbolIcon.objectForeground": "#cf7c9f",
    "symbolIcon.propertyForeground": "#92b4b1",
    "symbolIcon.stringForeground": "#53ab62",
    "symbolIcon.variableForeground": "#92b4b1",

    "editorIndentGuide.activeBackground": "#454545",
    "editorIndentGuide.background": "#2d2d2d",


    "panelTitle.inactiveForeground": "#e7e7e799",
    "panelTitle.activeForeground": "#92a3b1",
    "panelTitle.activeBorder": "#92a3b1",
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment", "punctuation.definition.generic"],
      "settings": {
        "foreground": "#7f8c98"
      }
    },
    {
      "name": "String Quoted",
      "scope": ["string.quoted"],
      "settings": {
        "foreground": "#fe8170"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "storage.type.function.arrow",
        "keyword.operator.assignment",
        "variable.other.readwrite",
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#ffffffcb"
      }
    },
    {
      "name": "Punctuation Separator",
      "scope": [
        "punctuation.separator.inheritance",
        "meta.class meta.function-call",
        "keyword.other.array.phpdoc",
        "bracket.round meta.function-call",
        "comment.block.documentation punctuation.definition.bracket.curly"
      ],
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ff8c69"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.other",
        "keyword.operator.new",
        "storage.type",
        "storage.modifier",
        "variable.language.this",
        "variable.language.this punctuation.definition.variable.php",
        "variable.language.super",
        "keyword.other.typedef",
        "constant.language",
        "constant.other",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#fe7ab2"
      }
    },
    {
      "name": "Comment Doc Keyword",
      "scope": ["keyword.other.phpdoc", "comment.block.documentation storage.type.class"],
      "settings": {
        "foreground": "#DD9AC5"
      }
    },
    {
      "name": "Class Declaration",
      "scope": ["entity.name.type.class"],
      "settings": {
        "foreground": "#6bdfffed"
      }
    },
    {
      "name": "Class Support",
      "scope": [
        "support.class",
        "support.type.property-name",
        "entity.name.type.module",
        "entity.other.inherited-class",
        "entity.name.class",
        "meta.return.type entity.name.type",
        "comment.block.documentation entity.name.type.instance",
        "entity.name.label"
      ],
      "settings": {
        "foreground": "#d9baff"
      }
    },
    {
      "name": "Class Methods",
      "scope": ["entity.name.function", "punctuation.definition.variable.php", "meta.type.annotation entity.name.type"],
      "settings": {
        "foreground": "#4DB1CB"
      }
    },
    {
      "name": "Class Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "entity.name.type.module"
      ],
      "settings": {
        "foreground": "#ffbf67e0"
      }
    },
    {
      "name": "Functions & Predefined Symbols",
      "scope": [
        "support.function",
        "variable.function",
        "keyword.other.special-method",
        "constant.other.php",
        "variable.other.constant.object"
      ],
      "settings": {
        "foreground": "#96b8e9"
      }
    },
    {
      "name": "Function Parameter (Arguments)",
      "scope": ["variable.other", "text.html.vue-html variable.other.readwrite"],
      "settings": {
        "foreground": "#6cc1c1"
      }
    },
    {
      "name": "HTML Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#fe7ab2"
      }
    },
    {
      "name": "HTML Tag Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ffbf67e0"
      }
    }
  ]
}
